				<Connection>
					<ModuleFrom>str_sscc</ModuleFrom>
					<EventFrom>StringBuilderEvent</EventFrom>
					<EventTo>AddParameterEvent</EventTo>
					<Mappings />
				</Connection>
			</Connections>
		</Module>
	</Modules>
	<Settings>
		<Setting>
			<Name>tcp_lsis422i</Name>
			<ModuleSetting>
				<TCPSettings>
					<Port>17050</Port>
					<ReconnectTimeout>5000</ReconnectTimeout>
					<IsServer>true</IsServer>
					<EndOfMessageInput>&#x0d;&#x0a;</EndOfMessageInput>
					<EndOfMessageOutput>&#x0d;&#x0a;</EndOfMessageOutput>
					<MaxLengthMessage>1024</MaxLengthMessage>
					<PauseWriterThresholdSize>1024</PauseWriterThresholdSize>
					<ResumeWriteThresholdSize>1024</ResumeWriteThresholdSize>
					<RemoteIP>192.168.0.98</RemoteIP>
					<EncodingNameInput>UTF-8</EncodingNameInput>
					<EncodingNameOutput>UTF-8</EncodingNameOutput>
				</TCPSettings>
			</ModuleSetting>
		</Setting>
		<Setting>
			<Name>regex_barcode_empty</Name>
			<ModuleSetting>
				<RegexMatcherSettings>
					<Keys>
						<string>barcode</string>
						<string>rest</string>
					</Keys>
					<Pattern>(?&lt;barcode&gt;[0-9]+);(?&lt;rest&gt;[0-9;]+)</Pattern>
					<RegexOptions>None</RegexOptions>
				</RegexMatcherSettings>
			</ModuleSetting>
		</Setting>
		<Setting>
			<Name>double_barcode</Name>
			<ModuleSetting>
				<RemovesDoubleSettings>
					<Delay>3000</Delay>
					<SendCountReport>false</SendCountReport>
					<FilterNull>true</FilterNull>
				</RemovesDoubleSettings>
			</ModuleSetting>
		</Setting>
		<Setting>
			<Name>str_barcode</Name>
			<ModuleSetting>
				<StringBuilderSettings>
					<DateFormat>o</DateFormat>
					<KeyWordStartIndicator>\$</KeyWordStartIndicator>
					<KeyWordEndIndicator>\;</KeyWordEndIndicator>
					<KeyWordStartIndicatorSystemFields>\+</KeyWordStartIndicatorSystemFields>
					<KeyWordEndIndicatorSystemFields>\-</KeyWordEndIndicatorSystemFields>
					<LocalDate>false</LocalDate>
					<Template>{"Timestamp": "+GetDate-" , "DeviceId":"washer", "Grai":"$BARCODE;"}</Template>
				</StringBuilderSettings>
			</ModuleSetting>
		</Setting>
		<Setting>
			<Name>msq_rabbit</Name>
			<ModuleSetting>
				<Amqp09Settings>
					<CheckConnectionTimeout>5000</CheckConnectionTimeout>
					<ConsumerTag>hertz_washer</ConsumerTag>
					<Uri>amqp://guest:guest@127.0.0.1:5672/</Uri>
					<Exchange>eprw_hertz_washer</Exchange>
					<DeclareExchange>false</DeclareExchange>
					<QueueName/>
					<DeclareQueue>false</DeclareQueue>
					<RoutingKeys/>
					<DefaultSendingRoutingKey>sscc.washing.grairead</DefaultSendingRoutingKey>
					<IncludeHeaders>true</IncludeHeaders>
					<Headers>
						<HeaderItem Key="ATS_TraceId" ValueIsPropKey="true">ID</HeaderItem>
					</Headers>
				</Amqp09Settings>
			</ModuleSetting>
		</Setting>
		<Setting>
			<Name>add_routingkey_grai</Name>
			<ModuleSetting>
				<AddParameterSettings>
					<ValuesToAdd>
						<AddParameterValue Key="RoutingKey">sscc.washing.grairead</AddParameterValue>
					</ValuesToAdd>
				</AddParameterSettings>
			</ModuleSetting>
		</Setting>
