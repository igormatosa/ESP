				<Connection>
					<ModuleFrom>hexconvert_rfid</ModuleFrom>
					<EventFrom>ConvertFromHexEvent</EventFrom>
					<EventTo>StringBuilderEvent</EventTo>
					<Mappings />
				</Connection>
			</Connections>
		</Module>
