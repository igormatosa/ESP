<?xml version="1.0" encoding="utf-8"?>
<ModuleConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Modules>
		<Module>
			<Name>tcp_lsis422i</Name>
			<Type>Aucxis.Hertz.Edge.Tcp.TCPModule</Type>
			<Connections />
		</Module>
		<Module>
			<Name>regex_barcode_empty</Name>
			<Type>Aucxis.Hertz.Edge.RegexMatcher.RegexMatcherModule</Type>
			<Connections>
				<Connection>
					<ModuleFrom>tcp_lsis422i</ModuleFrom>
					<EventFrom>TCPCommunicationEvent</EventFrom>
					<EventTo>RegexMatcherEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>CONTENT</InKey>
							<OutKey>StringToParse</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
				<Connection>
					<ModuleFrom>regex_barcode_empty</ModuleFrom>
					<EventFrom>MatchEvent</EventFrom>
					<EventTo>RegexMatcherEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>REST</InKey>
							<OutKey>StringToParse</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
			</Connections>
		</Module>
		<Module>
			<Name>double_barcode</Name>
			<Type>Aucxis.Hertz.Edge.RemovesDouble.RemovesDoubleModule</Type>
			<Connections>
				<Connection>
					<ModuleFrom>regex_barcode_empty</ModuleFrom>
					<EventFrom>MatchEvent</EventFrom>
					<EventTo>FilterEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>BARCODE</InKey>
							<OutKey>FILTERARGUMENT</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
			</Connections>
		</Module>
		<Module>
			<Name>str_barcode</Name>
			<Type>Aucxis.Hertz.Edge.StringBuilder.StringBuilderModule</Type>
			<Connections>
				<Connection>
					<ModuleFrom>double_barcode</ModuleFrom>
					<EventFrom>FilterEvent</EventFrom>
					<EventTo>StringBuilderEvent</EventTo>
					<Mappings />
				</Connection>
			</Connections>
		</Module>   
		<Module>
			<Name>add_routingkey_grai</Name>
			<Type>Aucxis.Hertz.Edge.AddParameter.AddParameterModule</Type>
			<Connections>
				<Connection>
					<ModuleFrom>str_barcode</ModuleFrom>
					<EventFrom>StringBuilderEvent</EventFrom>
					<EventTo>AddParameterEvent</EventTo>
					<Mappings />
				</Connection>
			</Connections>
		</Module>
		<Module>
			<Name>msq_rabbit</Name>
			<Type>Aucxis.Hertz.Edge.Amqp09.Amqp09Module</Type>
			<Connections>
				<Connection>
					<ModuleFrom>add_routingkey_grai</ModuleFrom>
					<EventFrom>AddParameterEvent</EventFrom>
					<EventTo>MessageQueueEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>RESPONSE</InKey>
							<OutKey>CONTENT</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
				<Connection>
					<ModuleFrom>add_routingkey_sscc</ModuleFrom>
					<EventFrom>AddParameterEvent</EventFrom>
					<EventTo>MessageQueueEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>RESPONSE</InKey>
							<OutKey>CONTENT</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
			</Connections>
		</Module>
		<Module>
			<Name>impinj</Name>
			<Type>Aucxis.Hertz.Edge.Impinj.ImpinjModule</Type>
			<Connections/>
		</Module>
		<Module>
			<Name>double_rfid</Name>
			<Type>Aucxis.Hertz.Edge.RemovesDouble.RemovesDoubleModule</Type>
			<Connections>
				<Connection>
					<ModuleFrom>impinj</ModuleFrom>
					<EventFrom>RfidReadEvent</EventFrom>
					<EventTo>FilterEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>TAG</InKey>
							<OutKey>FILTERARGUMENT</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
			</Connections>
		</Module>
		<Module>
			<Name>hexconvert_rfid</Name>
			<Type>Aucxis.Hertz.Edge.HexConverter.HexConvertorModule</Type>
			<Connections>
				<Connection>
					<ModuleFrom>double_rfid</ModuleFrom>
					<EventFrom>FilterEvent</EventFrom>
					<EventTo>ConvertFromHexEvent</EventTo>
					<Mappings>
						<Mapping>
							<InKey>TAG</InKey>
							<OutKey>HEX</OutKey>
						</Mapping>
					</Mappings>
				</Connection>
			</Connections>
		</Module>
		<Module>
			<Name>str_sscc</Name>
			<Type>Aucxis.Hertz.Edge.StringBuilder.StringBuilderModule</Type>
